{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "**/**.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "**/PublicAPI.*.txt",
            "_site/**"
          ]
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "EnumSortOrder": "alphabetic",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "globalMetadata": {
      "_appName": "LocalSqsSnsMessaging",
      "_appTitle": "LocalSqsSnsMessaging",
      "_appLogoPath": "icon.png",
      "_appFaviconPath": "icon.png",
      "_enableSearch": true,
      "_lang": "en"
    },
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/README.md"
        ]
      },
      {
        "files": [
          "**.md",
          "**.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "icon.png"
        ]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "template"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}
