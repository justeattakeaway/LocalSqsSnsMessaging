{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "labels": [
    "dependencies"
  ],
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "AWSSDK.SimpleNotificationService",
        "AWSSDK.SQS"
      ],
      "enabled": false
    },
    {
      "groupName": "Aspire",
      "matchPackageNames": [
        "/Aspire*/"
      ]
    },
    {
      "groupName": "TUnit",
      "matchPackageNames": [
        "/TUnit*/"
      ]
    }
  ]
}
