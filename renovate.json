{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "labels": [
    "dependencies"
  ],
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["AWSSDK.SimpleNotificationService", "AWSSDK.SQS"],
      "enabled": false
    },
    {
      "matchPackagePatterns": [
        "Aspire*"
      ],
      "groupName": "Aspire"
    }
  ]
}
